<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
 <class name="com.nt.entity.MobilePhoneUser" table="MOBILEPHONE_USER">
  <id name="regNo" > <!-- Singular ID field cfg -->
    <generator class="increment"/>
    </id>
   <version name="updateCount"/>
  <property name="mobileNo" />
  <property name="prepaid" />
  <property name="circle"/>
  <property name="callerTune" />
 </class>
</hibernate-mapping>